/* component/msg.wxss */
.mui-msg{width: 100%;display: flex;align-items: center;justify-content: center;flex-direction: column;}
.mui-msg__icon-area{margin-top: 62rpx;}
.mui-msg__icon-area image{ width: 200rpx;height: 200rpx;}

.mui-msg__text-area{margin-bottom: 70rpx;}
.mui-msg__text-area .mui-msg__title{
    font-size: 36rpx;color: #000000;
    margin-top: 48rpx;
    text-align: center;
}
.mui-msg__text-area .mui-msg__desc{
    font-size: 28rpx;color: #aaaaaa;margin-top: 30rpx;text-align: center;
}
.mui-msg__opr-area{
    width: 100%;display: flex;align-items: center;justify-content: space-between;
    margin: 0 60rpx;
}

.mui-btn-area{
    width: 298rpx;height: 68rpx;font-size: 36rpx;color: #1195db;line-height: 68rpx;border:1rpx solid #1195db;
    border-radius: 16rpx;text-align: center;background-color: #ffffff;
    margin-left: 16rpx;margin-right: 16rpx;
}
.mui-nav-hover{
    background-color: rgba(17, 149, 279, 0.1); opacity: 0.6;color: #555555;
}
